<h2>Search Author</h2>
<form action="/authors">
  <input type="text" name="name" value="<%= searchOption %> " />
  <button type="submit">Search</button>
</form>
<br />
<% if(authors.length!==0) { %> <% authors.forEach(author=>{ %>
<div><%= author.name %></div>
<% }) %><% } else { %>
<p>we don't have any author for now</p>
<% } %>
